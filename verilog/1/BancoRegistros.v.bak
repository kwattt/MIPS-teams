module BancoRegistros (
	input [4:0]control,
	input [3:0]readReg1,
	input [3:0]readReg2,
	input [3:0]writeReg,
	input [15:0]sign-ex,
	
	output reg [31:0]readData1,
	output reg [31:0]readData2,
);


reg [31:0]almacenBr[0:31];


`timescale 1ns/1ns
initial
begin
	$readmemb("C:\\Computacion\\almacenBr.txt", almacenBr,0,31);
	#10;
end

always @* // (control, readReg1, readReg2, writeReg, sign-ex)
begin
	
	almacenBr[writeReg] <= sign-ex;
	
	readData1 <= almacenBr[readReg1];
	readData2 <= almacenBr[readReg2];
	
end


